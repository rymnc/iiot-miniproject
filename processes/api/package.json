    "fastify-sensible": "^3.1.0",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/tap": "^15.0.0",
    "concurrently": "^6.0.0",
